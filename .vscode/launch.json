	"version": "0.2.0",
	"configurations": [
	{
		"name": "Launch Web Server",
		"program": "${workspaceFolder}/formSite/projectServer/server.js",
		"request": "launch",
		"skipFiles": [
			"<node_internals>/**"
		],
		"type": "node"
	},
		{
			"type": "java",
			"name": "Launch Current File",
			"request": "launch",
			"mainClass": "${file}"
		},
		{
			"type": "java",
			"name": "Launch Main",
			"request": "launch",
			"mainClass": "com.makerspace.Main",
			"projectName": "Makerspace"
		}
	]
}